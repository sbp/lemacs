/* #define BUILD_SHARED_LIB */


/* 
 *
 *  You shouldn't need to edit anything below this point.
 *
 */

#if (ProjectX == 5) && defined(BUILD_SHARED_LIB)
# define DoSharedLib YES
# define DoNormalLib YES
# include <Library.tmpl>
#endif

GENERATED_SRC = blpsheet.c build.c classbr_ps.c ctreebr_ps.c debuggerps.c \
                editmode.c leb_psheet.c projectdisp.c projectps.c search.c \
                target.c
         SRCS = $(GENERATED_SRC)

         OBJS = blpsheet.o build.o classbr_ps.o ctreebr_ps.o debuggerps.o \
                editmode.o leb_psheet.o projectdisp.o projectps.o search.o \
                target.o

all:: $(OBJS)

clean:: 
	$(RM) $(GENERATED_SRC)

.SUFFIXES: .xd .xdc

blpsheet.c: blpsheet.xdc
	./fidget-widget $(@:.c=.xdc) create_breaklist_p_sheet     > $@
build.c:
	./fidget-widget $(@:.c=.xdc) create_build_dialog           > $@
classbr_ps.c: classbr_ps.xdc
	./fidget-widget $(@:.c=.xdc) create_class_browser_p_sheet  > $@
ctreebr_ps.c: ctreebr_ps.xdc
	./fidget-widget $(@:.c=.xdc) create_call_browser_p_sheet   > $@
debuggerps.c: debuggerps.xdc
	./fidget-widget $(@:.c=.xdc) create_debugger_p_sheet       > $@
editmode.c: editmode.xdc
	./fidget-widget $(@:.c=.xdc) create_editmode_dialog        > $@
leb_psheet.c: leb_psheet.xdc
	./fidget-widget $(@:.c=.xdc) create_le_browser_p_sheet     > $@
projectdisp.c:
	./fidget-widget $(@:.c=.xdc) create_project_display_dialog > $@
projectps.c: projectps.xdc
	./fidget-widget $(@:.c=.xdc) create_project_p_sheet        > $@
search.c: search.xdc
	./fidget-widget $(@:.c=.xdc) create_search_dialog          > $@
target.c: target.xdc
	./fidget-widget $(@:.c=.xdc) create_target_dialog          > $@

#ifdef BUILD_SHARED_LIB
# if (ProjectX == 4)
SharedLibraryObjectRule ()
# else   /* X != 4 */
LibraryObjectRule ()
# endif /* X != 4 */
#else  /* !BUILD_SHARED_LIB */
NormalLibraryObjectRule ()
#endif /* !BUILD_SHARED_LIB */

DependTarget ()
